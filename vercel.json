{
  "version": 2,
  "buildCommand": "cd Frontend && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "Frontend/.next",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
